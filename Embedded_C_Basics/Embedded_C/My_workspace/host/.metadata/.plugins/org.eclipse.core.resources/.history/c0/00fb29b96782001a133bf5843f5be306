#include <stdio.h>
#include "math.h"

int main(){
	int a=2, b=3;
	printf("Addition = %d", addition(a,b));
	printf("\nSubtraction = %d", subtraction(a,b));
	printf("\nMultiplication = %d", multiplication(a,b));
	printf("\nDivision = %f", division(a,b));
	return 0;
}
