#include<stdio.h>
#include<stdint.h>

struct carModel{
	uint32_t carNumber;
	uint32_t carPrice;
	uint32_t carMaxSpeed;
	float carWeight;
};

int main(){
	struct carModel carFord, carBMW;

	carFord.carNumber = 2021;
	carFord.carPrice = 15000;
	carFord.carMaxSpeed = 220;
	carFord.carWeight = 1330;

	carBMW = {.carNumber = 4031, .carPrice = 35000, .carMaxSpeed = 160, .carWeight = 1900.96};

	return 0;
}
