#include <stdio.h>
#include "math.h"

int main(){
	int a=2, b=3;
	printf("Addition = %d", addition(a,b));
	printf("Subtraction = %d", subtraction(a,b));
	printf("Multiplication = %d", multiplicaiton(a,b));
	printf("Division = %f", division(a,b));
	return 0;
}
