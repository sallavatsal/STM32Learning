#include<stdio.h>

int main(){
	char var = 100;
	char* pVar = &var;
	char var2;

	printf("Address of var = %d", var);
	var2 = *pVar;				// Dereferencing pVar and storing it to var2
	printf("Value of var2 = %d", var);
	*pVar = 65;
	printf("value of var = %d", var);

	getchar();

	return 0;
}
