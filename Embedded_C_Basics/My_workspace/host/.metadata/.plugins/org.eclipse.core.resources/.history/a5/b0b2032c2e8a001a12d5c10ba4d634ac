#include<stdio.h>
#include<stdint.h>

struct carModel{
	uint32_t carNumber;
	uint32_t carPrice;
	uint32_t carMaxSpeed;
	float carWeight;
};

int main(){
	struct carModel carFord = {.carNumber = 2021, .carPrice = 15000, .carMaxSpeed = 220, .carWeight = 1330};
	struct carModel carBMW = {.carNumber = 4031, .carPrice = 35000, .carMaxSpeed = 160, .carWeight = 1900.96};

	return 0;
}
