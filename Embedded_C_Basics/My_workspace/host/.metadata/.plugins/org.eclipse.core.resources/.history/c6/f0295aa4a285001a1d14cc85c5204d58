#include<stdio.h>

int main(){
	float _a1, _a2, _a3;
	float average;

	printf("Enter first number: ");
	scanf("%f", &_a1);
	printf("\nEnter second number: ");
	scanf("%f", &_a2);
	printf("\nEnter third number: ");
	scanf("%f", &_a3);

	average = (_a1+_a2+_a3)/3;
	printf("Average = %f", average);

	return 0;
}
